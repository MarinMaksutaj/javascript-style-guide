{
  "env": {
    "browser": true,
    "jquery": true
  },

  "rules": {
    "block-scoped-var": [0],
    "comma-dangle": [2],
    "comma-style": [2, "last"],
    "curly": [2],
    "dot-notation": [0],
    "eqeqeq": [2],
    "guard-for-in": [1],
    "indent": [2],
    "no-caller": [2],
    "new-cap": [2],
    "no-eq-null": [0],
    "no-eval": [2],
    "no-extend-native": [2],
    "no-loop-func": [0],
    "no-plusplus": [2],
    "no-shadow": [0],
    "no-trailing-spaces": [2],
    "no-undef": [2],
    "no-unused-expressions": [2],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "no-use-before-define": [2],
    "quotes": [2, "single", "avoid-escape"],
    "strict": [2, "global"],
  }
}
